{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "mark":  { "type": "bar"},
    "encoding": {
      "column": {
        "type": "ordinal", "spacing": 10
      },
      "y": {
        "type": "quantitative",
        "axis": {"grid": false}
      },
      "x": {
        "type": "nominal",
        "axis": {"title": ""}
      },
      "color": {
        "type": "nominal",
        "scale": {"range": ["#675193", "#ca8861"]}
      }
    },
    "config": {
      "view": {"stroke": "transparent"},
      "axis": {"domainWidth": 1}
    }
  }