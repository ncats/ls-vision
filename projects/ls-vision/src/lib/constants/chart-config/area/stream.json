{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "mark": { "type": "area"},
    "encoding": {
      "x": {
        "timeUnit": "yearmonth", "type": "temporal",
        "axis": {"domain": true, "tickSize": 0}
      },
      "y": {
        "aggregate": "sum", "type": "quantitative",
        "axis": null,
        "stack": "center"
      },
      "color": {"type":"nominal", "scale":{"scheme": "category20b"}}
    }
  }
  