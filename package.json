{
  "name": "@labshare/ls-vision",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build @labshare/ls-vision",
    "build:prod": "ng build @labshare/ls-vision --prod",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coverage": "ng test --codeCoverage=true --watch=false",
    "coverage:watch": "ng test --codeCoverage=true",
    "e2e": "ng e2e",
    "lint": "npm run prettier:check && ng lint",
    "lint:fix": "ng lint -- --fix && npm run prettier:fix",
    "prettier": "prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier -- -l",
    "prettier:cli": "npm run prettier -- -c",
    "prettier:fix": "npm run prettier:cli -- --write",
    "semantic-release": "semantic-release",
    "test": "ng test --watch=false"
  },
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/forms": "~9.1.12",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "lodash": "^4.17.19",
    "rxjs": "~6.6.2",
    "tslib": "^1.10.0",
    "vega": "^5.13.0",
    "vega-embed": "^6.10.0",
    "vega-lite": "^4.13.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular-devkit/build-ng-packagr": "~0.901.12",
    "@angular/cli": "~9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@labshare/semantic-release-config": "^1.1.3",
    "@semantic-release/exec": "^5.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.54",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^10.2.9",
    "ng-packagr": "^9.0.2",
    "prettier": "^2.0.5",
    "protractor": "~5.4.0",
    "semantic-release": "^17.0.8",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,md,html}": "npm run lint:fix",
    "relative": true
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist"
        }
      ],
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "cp ./dist/package.json ./projects/ls-vision/package.json"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "projects/ls-vision/package.json"
          ]
        }
      ]
    ]
  }
}
